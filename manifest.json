{
    "manifest_version": 3,
    "name": "Text Processing Extension",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "contextMenus"
      
    ],
    "host_permissions": [
      "http://localhost:8000/"
    ],
    "background": {
      "service_worker": "background.js"
    },

    "action": {
        "default_popup": "popup.html"
     }

}